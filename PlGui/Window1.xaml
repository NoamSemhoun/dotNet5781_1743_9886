<Window x:Class="PlGui.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlGui"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    
    <Grid>
        <local:DoubleTextBox Height="50" x:Name="aa" Text="{Binding Path=Time}"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="35" Margin="50,345,0,0" VerticalAlignment="Top" Width="120" Click="Button_Click"/>
    </Grid>
</Window>
